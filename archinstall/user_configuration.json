{
  "additional-repositories": [],
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader": "systemd-bootctl",
  "config_version": "2.8.0",
  "hostname": "archlinux",
  "kernels": ["linux"],
  "locale_config": {
    "kb_layout": "us",
    "sys_enc": "UTF-8",
    "sys_lang": "en_US.UTF-8"
  },
  "mirror_config": {
    "custom_mirrors": [],
    "mirror_regions": {
      "Worldwide": [
        "https://geo.mirror.pkgbuild.com/$repo/os/$arch"
      ]
    }
  },
  "network_config": {
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "packages": [
    "vim",
    "git",
    "htop",
    "wget",
    "curl"
  ],
  "parallel downloads": 5,
  "profile_config": {
    "gfx_driver": null,
    "greeter": null,
    "profile": {
      "details": [],
      "main": "minimal"
    }
  },
  "swap": true,
  "timezone": "UTC",
  "uki": false
}
